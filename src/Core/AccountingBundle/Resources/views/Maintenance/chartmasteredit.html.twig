{# src/Core/AccountingBundle/Resources/views/Maintenance/chartmasteredit.html.twig #}
{% extends 'CoreAccountingBundle:Default:accounting.html.twig' %}

{% block title %}Accounting - Chart of Accounts{% endblock %}

{% block navigation %}
    {{ parent() }}
{% endblock %}

{% block headtitle %}
Edit Chart of Accounts
{% endblock %}

{% block body %}
<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				Edit Accounts
			</div>
			<!-- /.panel-heading -->
			<div class="panel-body">
            	<div class="row">
                	<div class="col-lg-6">
                    	{{ form_start(form) }}
                    		<div class="form-group">
                    		
                    		
                            	<label>Account Code</label>
                                <input class="form-control" id="Accountcode">
                            </div>


<form action="{{ path('CoreAccountingBundle_maintenance_chartmaster_edit',{'account_id':accountcode_id}) }}" 
		method="post" {{ form_enctype(form) }} >
    {{ form_widget(form, {'attr': {'class':'form-group'}}) }}
</form>

{% endblock %}